{
    "name": "iad/db-decoration-bundle",
    "description": "Decorate database data from custom entity annotation class",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrien MALLET",
            "email": "adrien.mallet@iadinternational.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
	    "psr-4": {
		    "Iad\\DbDecorationBundle\\": ""
	    }
    },
    "autoload-dev": {
        "psr-4": {
		    "Iad\\Test\\DbDecorationBundle\\": "Test/"
        }
    },
    "require": {
        "symfony/http-kernel": "4.2.*",
        "symfony/dependency-injection": "4.2.*",
        "doctrine/annotations": "^1.8"
    },
    "require-dev": {
        "symfony/framework-bundle": "4.2.*",
        "symfony/yaml": "4.2.*",
        "symfony/console": "4.2.*",
        "symfony/process": "4.2.*"
    }
}
